{
  "name": "eslint-plugin-smile",
  "version": "0.2.2",
  "description": "Smile ESLint rules for js, ts, react, vue, angular, next...",
  "homepage": "https://github.com/Smile-SA/eslint-plugin-smile",
  "repository": {
    "type": "git",
    "url": "git@github.com:Smile-SA/eslint-plugin-smile.git"
  },
  "bugs": {
    "url": "https://github.com/Smile-SA/eslint-plugin-smile/issues"
  },
  "main": "dist/index.js",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "index.js",
    "dist",
    "rules"
  ],
  "scripts": {
    "build": "rollup index.js --file dist/index.js --format cjs",
    "eslint": "eslint . --ignore-path .gitignore --fix",
    "prepare": "npm run build",
    "changelog": "changelog"
  },
  "keywords": [
    "eslint",
    "typescript",
    "react",
    "vue",
    "angular",
    "next"
  ],
  "author": {
    "name": "Tony Cabaye",
    "email": "tonai59+github@gmail.com",
    "url": "https://github.com/tonai"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-cypress": "^2.14.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-testing-library": "^6.0.1"
  },
  "devDependencies": {
    "generate-changelog": "^1.8.0",
    "rollup": "^3.29.0"
  },
  "peerDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.49.0",
    "eslint-config-next": "^13.4.19",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-vue": "^9.17.0"
  },
  "peerDependenciesMeta": {
    "@rushstack/eslint-patch": {
      "optional": true
    },
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "@vue/eslint-config-prettier": {
      "optional": true
    },
    "@vue/eslint-config-typescript": {
      "optional": true
    },
    "eslint-config-next": {
      "optional": true
    },
    "eslint-plugin-jest": {
      "optional": true
