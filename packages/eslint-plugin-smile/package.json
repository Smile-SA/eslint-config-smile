{
  "name": "eslint-plugin-smile",
  "version": "0.0.0",
  "description": "Smile ESLint rules for js, ts, react, vue, angular, next...",
  "homepage": "https://github.com/Smile-SA/eslint-plugin-smile",
  "repository": {
    "type": "git",
    "url": "git@github.com:Smile-SA/eslint-plugin-smile.git"
  },
  "bugs": {
    "url": "https://github.com/Smile-SA/eslint-plugin-smile/issues"
  },
  "main": "dist/index.js",
  "files": [
    "LICENSE",
    "README.md",
    "index.js",
    "dist",
    "rules"
  ],
  "scripts": {
    "build": "rollup index.js --file dist/index.js --format cjs",
    "eslint": "eslint . --ignore-path .gitignore --fix",
    "release:major": "changelog -M && git add CHANGELOG.md && git commit -m 'feat(docs): updated CHANGELOG.md' && npm version major && git push origin && git push origin --tags",
    "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m 'feat(docs): updated CHANGELOG.md' && npm version minor && git push origin && git push origin --tags",
    "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m 'feat(docs): updated CHANGELOG.md' && npm version patch && git push origin && git push origin --tags"
  },
  "keywords": [
    "eslint",
    "typescript",
    "react",
    "vue",
    "angular",
    "next"
  ],
  "author": {
    "name": "Tony Cabaye",
    "email": "tonai59+github@gmail.com",
    "url": "https://github.com/tonai"
  },
  "license": "GPL",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "eslint-plugin-import": "^2.27.5",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1"
  },
  "devDependencies": {
    "generate-changelog": "^1.8.0",
    "rollup": "^3.12.0"
  },
  "peerDependencies": {
    "eslint": "^8.33.0",
    "eslint-config-next": "^13.1.6",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-vue": "^9.9.0",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vue/eslint-config-typescript": "^11.0.2"
  },
  "peerDependenciesMeta": {
    "eslint-config-next": {
      "optional": true
    },
    "eslint-config-react-app": {
      "optional": true
    },
    "eslint-import-resolver-typescript": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "@vue/eslint-config-typescript": {
      "optional": true
    }
  }
}
